<div  style="float: left;">
  <%= link_to "Back to expensesSheet", expenses_sheet_url(@expensesSheet), class: "btn btn-primary" %>
</div>
<h1>
  fullcalendar
  <%= link_to "#{@expensesSheet.name}", @expensesSheet, 'data-turbolinks': false %>
</h1>
<div id='calendar'></div>

<script>
  document.addEventListener("turbolinks:before-cache", function() {
    $('#calendar').fullCalendar('destroy');
  })
  document.addEventListener("turbolinks:load", function() {
    $('#calendar').fullCalendar({
      firstDay: 1,
      events: <%= raw @expenses_calendar.to_json() %>
    });
  })
</script>